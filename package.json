{
  "name": "mc-status-card",
  "version": "0.2.6",
  "private": true,
  "description": "Generate a status card for a Minecraft server",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "node",
    "minecraft",
    "status-card"
  ],
  "author": "ppodds",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.3",
    "canvas": "^2.9.3",
    "minecraft-protocol": "^1.35.0"
  },
  "devDependencies": {
    "@types/node": "^16.10.5",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.47.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.19.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
